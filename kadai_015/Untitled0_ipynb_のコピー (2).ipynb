        "#クラス課題\n",
        "class Human:\n",
        "    # コンストラクタを定義する\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "    def printinfo (self):\n",
        "        print(f\"名前は{self.name}です。年齢は{self.age}歳です。\")\n",
        "\n",
        "# インスタンス化する\n",
        "human = Human(\"侍太郎\", 36,)\n",
        "\n",
        "# 属性にアクセスし、値を出力する\n",
        "human.printinfo()"
      ],
